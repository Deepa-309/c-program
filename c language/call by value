#include<stdio.h>
int swap(int x,int y)//fun definition
{
    int temp;
    temp=x;
    x=y;
    y=temp;
    printf("After swapping x=%d\ty=%d\n",x,y);
}
int main()
{
    int a,b;
    a=10;
    b=20;
    printf("Before swapping a=%d\tb=%d\n",a,b);
    swap (a,b)//funcalling
    printf("After swapping a=%d\tb=%d\n",a,b);
    return 0;
}
